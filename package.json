{
  "name": "learning-management-backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "migration:run": "node ace migration:run",
    "migration:rollback": "node ace migration:rollback",
    "migration:fresh": "node ace migration:fresh",
    "db:seed": "node ace db:seed",
    "db:setup": "npm run migration:fresh && npm run db:seed",
    "queue:start": "node --no-warnings ace queue:listen --queue=startQuiz",
    "queue:start_notification": "node --no-warnings ace queue:listen --queue=makeQuizSuccess"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#domains/*": "./app/Domains/*.js",
    "#jobs/*": "./app/Jobs/*.js",
    "#shared/*": "./app/Shared/*.js",
    "#commands/*": "./commands/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.4",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.11.24",
    "@types/node": "^22.15.18",
    "eslint": "^9.26.0",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-node-maintained": "^10.9.5",
    "typescript": "~5.8"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.4.2",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/limiter": "^2.4.0",
    "@adonisjs/lucid": "^21.8.0",
    "@adonisjs/validator": "^13.0.2",
    "@google/genai": "^1.19.0",
    "@google/generative-ai": "^0.24.1",
    "@rlanz/bull-queue": "^3.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "adonisjs-scheduler": "^2.4.1",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "bullmq": "^5.58.5",
    "cors": "^2.8.5",
    "groq-sdk": "^0.32.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "mongoose-aggregate-paginate-v2": "^1.1.4",
    "mongoose-paginate-v2": "^1.9.1",
    "node-cron": "^4.2.1",
    "openai": "^5.19.1",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "zod": "^4.0.17"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
}
